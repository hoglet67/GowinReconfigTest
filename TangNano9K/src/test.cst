// System Clock (27MHz)
IO_LOC  "sys_clk" 52;
IO_PORT "sys_clk" IO_TYPE=LVCMOS33 PULL_MODE=UP;

// Button 1
IO_LOC  "btn1_n" 3;
IO_PORT "btn1_n" PULL_MODE=UP;

// Button 2
IO_LOC  "btn2_n" 4;
